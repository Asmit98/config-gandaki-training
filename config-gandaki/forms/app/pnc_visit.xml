          <delivery_date_formatted tag="hidden"/>
          <delivery_date_formatted_ne/>
          <days_since_delivery tag="hidden"/>
          <current_visit_count tag="hidden"/>
          <current_visit_count_ne/>
          <context_vars tag="hidden">

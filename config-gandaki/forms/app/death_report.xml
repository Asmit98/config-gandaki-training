      <bind nodeset="/death_report/inputs" relevant="./source='user'"/>
      <bind nodeset="/death_report/inputs/date_id" type="string"/>
      <bind nodeset="/death_report/inputs/source" type="string"/>
      <bind nodeset="/death_report/inputs/source_id" type="string"/>
      <bind nodeset="/death_report/inputs/user/contact_id" type="db:person"/>
      <bind nodeset="/death_report/inputs/contact/_id" type="db:person" required="true()"/>
      <bind nodeset="/death_report/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/death_report/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/death_report/inputs/contact/first_name" type="string"/>
      <bind nodeset="/death_report/inputs/contact/last_name" type="string"/>
      <bind nodeset="/death_report/inputs/contact/name" type="string"/>
      <bind nodeset="/death_report/inputs/contact/sex" type="string"/>
      <bind nodeset="/death_report/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/death_report/birthdate" type="string" calculate="../inputs/contact/date_of_birth"/>
      <bind nodeset="/death_report/patient_first_name" type="string" calculate="../inputs/contact/first_name"/>
      <bind nodeset="/death_report/patient_last_name" type="string" calculate="../inputs/contact/last_name"/>
      <bind nodeset="/death_report/patient_full_name" type="string" calculate="concat( /death_report/patient_first_name , ' ',  /death_report/patient_last_name )"/>
      <bind nodeset="/death_report/patient_name" type="string" calculate="coalesce(../inputs/contact/name,  /death_report/patient_full_name )"/>
      <bind nodeset="/death_report/patient_age_years" type="string" calculate="floor( difference-in-months(  /death_report/birthdate , today() ) div 12 )"/>
      <bind nodeset="/death_report/patient_age_months" type="string" calculate="difference-in-months(  /death_report/birthdate , now() ) -  /death_report/patient_age_years *12"/>
      <bind nodeset="/death_report/patient_age_days" type="string" calculate="floor( decimal-date-time(now()) - decimal-date-time( /death_report/birthdate ) )"/>
      <bind nodeset="/death_report/death_detail/death_date" type="date" constraint=". &lt;= now() and . &gt;=  /death_report/birthdate " jr:constraintMsg="jr:itext('/death_report/death_detail/death_date:jr:constraintMsg')" required="true()"/>
      <bind nodeset="/death_report/death_detail/death_date_nepali" type="string" calculate="to-bikram-sambat( /death_report/death_detail/death_date )"/>
      <bind nodeset="/death_report/death_detail/name_report_death" type="string" required="true()"/>
      <bind nodeset="/death_report/death_detail/relation_with_death_person" type="string" required="true()"/>
      <bind nodeset="/death_report/death_detail/place_death" type="select1" required="true()"/>
      <bind nodeset="/death_report/death_detail/place_death_other" type="string" relevant=" /death_report/death_detail/place_death  = 'other'"/>
      <bind nodeset="/death_report/death_detail/cause_death" type="select1" required="true()"/>
      <bind nodeset="/death_report/death_detail/cause_maternal_death" type="select1" relevant=" /death_report/death_detail/cause_death  = 'maternal'"/>
      <bind nodeset="/death_report/death_detail/cause_death_other" type="string" relevant=" /death_report/death_detail/cause_death  = 'other' or  /death_report/death_detail/cause_maternal_death  = 'other'" required="true()"/>
      <bind nodeset="/death_report/death_detail/death_place" type="string" calculate="if( /death_report/death_detail/place_death ='other',  /death_report/death_detail/place_death_other , jr:choice-name( /death_report/death_detail/place_death , ' /death_report/death_detail/place_death '))"/>
      <bind nodeset="/death_report/death_detail/death_reason" type="string" calculate="if( /death_report/death_detail/cause_death ='other',  /death_report/death_detail/cause_death_other , if( /death_report/death_detail/cause_death ='maternal', jr:choice-name( /death_report/death_detail/cause_maternal_death , ' /death_report/death_detail/cause_maternal_death '), jr:choice-name( /death_report/death_detail/cause_death , ' /death_report/death_detail/cause_death ')))"/>
      <bind nodeset="/death_report/death_detail/did_death_registered" type="select1" required="true()"/>
      <bind nodeset="/death_report/death_detail/why_death_not_registered" type="string" relevant=" /death_report/death_detail/did_death_registered  = 'no'" required="true()"/>
      <bind nodeset="/death_report/patient_age_display_en" type="string" calculate="if( /death_report/patient_age_years  +  /death_report/patient_age_months  +  /death_report/patient_age_days  = 0, 
 '0 days', 
 concat(
 if( /death_report/patient_age_years  &gt; 0, concat(int( /death_report/patient_age_years ), ' years '), ''),
 if( /death_report/patient_age_months  &gt; 0, concat(int( /death_report/patient_age_months ), ' months '), ''),
 if( /death_report/patient_age_years  = 0 and  /death_report/patient_age_months  = 0 and  /death_report/patient_age_days  &gt; 0, 
 concat(int( /death_report/patient_age_days ), ' days'), 
 ''
 )
 )
)"/>
      <bind nodeset="/death_report/patient_age_ne" type="string" calculate="if( /death_report/patient_age_years  +  /death_report/patient_age_months  +  /death_report/patient_age_days  = 0, 
 '0 दिन', 
 concat(
 if( /death_report/patient_age_years  &gt; 0, concat(int( /death_report/patient_age_years ), ' वर्ष '), ''),
 if( /death_report/patient_age_months  &gt; 0, concat(int( /death_report/patient_age_months ), ' महिना '), ''),
 if( /death_report/patient_age_years  = 0 and  /death_report/patient_age_months  = 0 and  /death_report/patient_age_days  &gt; 0, 
 concat(int( /death_report/patient_age_days ), ' दिन'), 
 ''
 )
 )
)"/>
      <bind nodeset="/death_report/patient_age_display_ne" type="string" calculate="translate( /death_report/patient_age_ne ,'1234567890','१२३४५६७८९०')"/>
      <bind nodeset="/death_report/summary/submit_note" readonly="true()" type="string"/>
      <bind nodeset="/death_report/summary/summary_0" readonly="true()" type="string"/>
      <bind nodeset="/death_report/summary/summary_1" readonly="true()" type="string"/>
      <bind nodeset="/death_report/summary/summary_2" readonly="true()" type="string"/>
      <bind nodeset="/death_report/summary/summary_3" readonly="true()" type="string"/>
      <bind nodeset="/death_report/summary/summary_4" readonly="true()" type="string"/>
      <bind nodeset="/death_report/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/death_report/inputs">
      <label ref="jr:itext('/death_report/inputs:label')"/>
      <input appearance="hidden" ref="/death_report/inputs/source">
        <label ref="jr:itext('/death_report/inputs/source:label')"/>
      </input>
      <input appearance="hidden" ref="/death_report/inputs/source_id">
        <label ref="jr:itext('/death_report/inputs/source_id:label')"/>
      </input>
      <group ref="/death_report/inputs/user">
        <input appearance="db-object" ref="/death_report/inputs/user/contact_id"/>
      </group>
      <group ref="/death_report/inputs/contact">
        <input appearance="db-object" ref="/death_report/inputs/contact/_id"/>
        <input ref="/death_report/inputs/contact/date_of_birth"/>
        <input ref="/death_report/inputs/contact/first_name"/>
        <input ref="/death_report/inputs/contact/last_name"/>
        <input appearance="hidden" ref="/death_report/inputs/contact/name"/>
        <input ref="/death_report/inputs/contact/sex"/>
      </group>
    </group>
    <group appearance="field-list" ref="/death_report/death_detail">
      <label ref="jr:itext('/death_report/death_detail:label')"/>
      <input ref="/death_report/death_detail/death_date">
        <label ref="jr:itext('/death_report/death_detail/death_date:label')"/>
      </input>
      <input ref="/death_report/death_detail/name_report_death">
        <label ref="jr:itext('/death_report/death_detail/name_report_death:label')"/>
      </input>
      <input ref="/death_report/death_detail/relation_with_death_person">
        <label ref="jr:itext('/death_report/death_detail/relation_with_death_person:label')"/>
      </input>
      <select1 ref="/death_report/death_detail/place_death">
        <label ref="jr:itext('/death_report/death_detail/place_death:label')"/>
        <item>
          <label ref="jr:itext('/death_report/death_detail/place_death/hf_hospital:label')"/>
          <value>hf_hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/place_death/home:label')"/>
          <value>home</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/place_death/on_the_way_hf:label')"/>
          <value>on_the_way_hf</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/place_death/other:label')"/>
          <value>other</value>
        </item>
      </select1>
      <input ref="/death_report/death_detail/place_death_other">
        <label ref="jr:itext('/death_report/death_detail/place_death_other:label')"/>
      </input>
      <select1 ref="/death_report/death_detail/cause_death">
        <label ref="jr:itext('/death_report/death_detail/cause_death:label')"/>
        <itemset nodeset="instance('death_causes')/root/item[name != 'maternal' or ( /death_report/inputs/contact/sex  = 'female' and  /death_report/patient_age_years  &gt; 12 and  /death_report/patient_age_years  &lt; 55)]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <select1 ref="/death_report/death_detail/cause_maternal_death">
        <label ref="jr:itext('/death_report/death_detail/cause_maternal_death:label')"/>
        <item>
          <label ref="jr:itext('/death_report/death_detail/cause_maternal_death/prolonged_labour:label')"/>
          <value>prolonged_labour</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/cause_maternal_death/unable_to_reach:label')"/>
          <value>unable_to_reach</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/cause_maternal_death/placenta_previa:label')"/>
          <value>placenta_previa</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/cause_maternal_death/lie_presentation:label')"/>
          <value>lie_presentation</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/cause_maternal_death/others:label')"/>
          <value>others</value>
        </item>
      </select1>
      <input ref="/death_report/death_detail/cause_death_other">
        <label ref="jr:itext('/death_report/death_detail/cause_death_other:label')"/>
      </input>
      <select1 ref="/death_report/death_detail/did_death_registered">
        <label ref="jr:itext('/death_report/death_detail/did_death_registered:label')"/>
        <item>
          <label ref="jr:itext('/death_report/death_detail/did_death_registered/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/death_report/death_detail/did_death_registered/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/death_report/death_detail/why_death_not_registered">
        <label ref="jr:itext('/death_report/death_detail/why_death_not_registered:label')"/>
        <hint ref="jr:itext('/death_report/death_detail/why_death_not_registered:hint')"/>
      </input>
    </group>
    <group appearance="field-list summary" ref="/death_report/summary">
      <input ref="/death_report/summary/submit_note">
        <label ref="jr:itext('/death_report/summary/submit_note:label')"/>
      </input>
      <input appearance="h1 yellow" ref="/death_report/summary/summary_0">
        <label ref="jr:itext('/death_report/summary/summary_0:label')"/>
      </input>
      <input ref="/death_report/summary/summary_1">
        <label ref="jr:itext('/death_report/summary/summary_1:label')"/>
      </input>
      <input ref="/death_report/summary/summary_2">
        <label ref="jr:itext('/death_report/summary/summary_2:label')"/>
      </input>
      <input ref="/death_report/summary/summary_3">
        <label ref="jr:itext('/death_report/summary/summary_3:label')"/>
      </input>
      <input ref="/death_report/summary/summary_4">
        <label ref="jr:itext('/death_report/summary/summary_4:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
